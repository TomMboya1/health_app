<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="datesRowModel" type="com.health.app.modules.drdetails.data.model.DatesRowModel"/>
    </data>
    <LinearLayout android:gravity="start" android:id="@+id/linearMon"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="vertical"
        android:paddingBottom="@dimen/_10pxv"
        android:paddingEnd="@dimen/_11pxh"
        android:paddingStart="@dimen/_11pxh"
        android:paddingTop="@dimen/_10pxv" style="@style/groupStylenameprimarycornerRadius_1">
        <TextView android:ellipsize="end" android:id="@+id/txtMon"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{datesRowModel.txtMon}"
            style="@style/txtIntermedium10" tools:text="@string/lbl_mon"/>
        <TextView android:ellipsize="end" android:id="@+id/txtDate"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{datesRowModel.txtDate}"
            style="@style/txtIntersemibold18" tools:text="@string/lbl_21"/>
    </LinearLayout>
</layout>
